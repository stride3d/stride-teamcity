<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="79b51553-f315-4ae0-ace7-4c20c8ca7021" xsi:noNamespaceSchemaLocation="http://www.jetbrains.com/teamcity/schemas/2018.1/project-config.xsd">
  <name>Build Package</name>
  <description />
  <settings>
    <options>
      <option name="allowExternalStatus" value="true" />
      <option name="artifactRules" value="bin/packages/**/*.nupkg =&gt; bin/packages&#xA;build/Xenko.build =&gt; build&#xA;build/Xenko.version =&gt; build&#xA;build/.nuget/NuGet.exe =&gt; build/.nuget" />
    </options>
    <disabled-settings>
      <setting-ref ref="RUNNER_11" />
      <setting-ref ref="vcsTrigger" />
    </disabled-settings>
    <parameters>
      <param name="XenkoGraphicsApiDependentBuildAll" value="false" spec="checkbox uncheckedValue='false' display='normal' checkedValue='true'" />
      <param name="XenkoPlatforms" value="Windows" spec="select data_5='Linux' display='normal' multiple='true' valueSeparator=';' label='Platforms' data_2='UWP' data_1='Windows' data_4='Android' data_3='iOS'" />
      <param name="env.XenkoOfficialBuild" value="" spec="checkbox checkedValue='true'" />
    </parameters>
    <build-runners>
      <runner id="RUNNER_10" name="" type="MSBuild">
        <parameters>
          <param name="build-file-path" value="build\Xenko.build" />
          <param name="dotNetCoverage.NCover.HTMLReport.File.Sort" value="0" />
          <param name="dotNetCoverage.NCover.HTMLReport.File.Type" value="1" />
          <param name="dotNetCoverage.NCover.Reg" value="selected" />
          <param name="dotNetCoverage.NCover.platformBitness" value="x86" />
          <param name="dotNetCoverage.NCover.platformVersion" value="v2.0" />
          <param name="dotNetCoverage.NCover3.Reg" value="selected" />
          <param name="dotNetCoverage.NCover3.args" value="//ias .*" />
          <param name="dotNetCoverage.NCover3.platformBitness" value="x86" />
          <param name="dotNetCoverage.NCover3.platformVersion" value="v2.0" />
          <param name="dotNetCoverage.NCover3.reporter.executable.args" value="//or FullCoverageReport:Html:{teamcity.report.path}" />
          <param name="dotNetCoverage.PartCover.Reg" value="selected" />
          <param name="dotNetCoverage.PartCover.includes" value="[*]*" />
          <param name="dotNetCoverage.PartCover.platformBitness" value="x86" />
          <param name="dotNetCoverage.PartCover.platformVersion" value="v2.0" />
          <param name="dotNetCoverage.dotCover.home.path" value="%teamcity.tool.JetBrains.dotCover.CommandLineTools.DEFAULT%" />
          <param name="msbuild_version" value="15.0" />
          <param name="run-platform" value="x86" />
          <param name="runnerArgs" value="/nr:false /p:XenkoPlatforms=&quot;%XenkoPlatforms%&quot; /p:XenkoGraphicsApiDependentBuildAll=%XenkoGraphicsApiDependentBuildAll%" />
          <param name="targets" value="Package" />
          <param name="teamcity.step.mode" value="default" />
          <param name="toolsVersion" value="15.0" />
        </parameters>
      </runner>
      <runner id="RUNNER_11" name="Update build number with actual package version" type="jetbrains_powershell">
        <parameters>
          <param name="jetbrains_powershell_execution" value="PS1" />
          <param name="jetbrains_powershell_noprofile" value="true" />
          <param name="jetbrains_powershell_script_code" value="&quot;##teamcity[buildNumber '{0}']&quot; -f (Get-Content .\Xenko.version) | Write-Host" />
          <param name="jetbrains_powershell_script_mode" value="CODE" />
          <param name="teamcity.build.workingDir" value="build" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="Xenko" />
    </vcs-settings>
    <requirements />
    <build-triggers>
      <build-trigger id="vcsTrigger" type="vcsTrigger">
        <parameters>
          <param name="enableQueueOptimization" value="true" />
          <param name="quietPeriodMode" value="DO_NOT_USE" />
        </parameters>
      </build-trigger>
    </build-triggers>
    <build-extensions>
      <extension id="BUILD_EXT_4" type="commit-status-publisher">
        <parameters>
          <param name="github_authentication_type" value="token" />
          <param name="github_host" value="https://api.github.com" />
          <param name="publisherId" value="githubStatusPublisher" />
          <param name="secure:github_access_token" value="credentialsJSON:df649119-0086-4cc6-bde6-e1fa7127681b" />
          <param name="vcsRootId" value="Xenko" />
        </parameters>
      </extension>
    </build-extensions>
    <cleanup />
  </settings>
</build-type>

