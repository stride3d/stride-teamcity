<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="3c15ea7d-af51-4f59-8b29-a288a4be6e41" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2021.1/project-config.xsd">
  <name>Deploy</name>
  <description />
  <settings>
    <options>
      <option name="allowPersonalBuildTriggering" value="false" />
      <option name="buildConfigurationType" value="DEPLOYMENT" />
      <option name="maximumNumberOfBuilds" value="1" />
    </options>
    <parameters>
      <param name="AzurePublishPassword" value="credentialsJSON:3cab913f-8023-41cb-82f7-91164e921ecb" spec="password display='prompt'" />
      <param name="AzurePublishProfile" value="stride-doc.PublishSettings" />
    </parameters>
    <build-runners>
      <runner id="RUNNER_45" name="Deploy documentation to Azure" type="jetbrains_powershell">
        <parameters>
          <param name="jetbrains_powershell_errorToError" value="true" />
          <param name="jetbrains_powershell_execution" value="PS1" />
          <param name="jetbrains_powershell_noprofile" value="true" />
          <param name="jetbrains_powershell_scriptArguments" value="&quot;%AzurePublishProfile%&quot; &quot;%AzurePublishPassword%&quot;" />
          <param name="jetbrains_powershell_script_file" value="build/deploy.ps1" />
          <param name="jetbrains_powershell_script_mode" value="FILE" />
          <param name="teamcity.build.workingDir" value="build" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="Documentation_StrideDocs" />
    </vcs-settings>
    <requirements />
    <build-triggers />
    <artifact-dependencies>
      <dependency id="ARTIFACT_DEPENDENCY_10" sourceBuildTypeId="Documentation_Build" cleanDestination="false">
        <revisionRule name="sameChainOrLastFinished" revision="latest.sameChainOrLastFinished" />
        <artifact sourcePath="*.zip =&gt; ." />
      </dependency>
    </artifact-dependencies>
    <dependencies>
      <depend-on sourceBuildTypeId="Documentation_Build">
        <options>
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
    </dependencies>
    <cleanup />
  </settings>
</build-type>

